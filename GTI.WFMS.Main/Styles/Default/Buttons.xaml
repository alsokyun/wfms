                        <Image Grid.Column="0" Source="/Resources/Images/Image/icon_user.png" Stretch="None"/>
                        <ContentPresenter Grid.Column="1" Margin="0" HorizontalAlignment="Left" VerticalAlignment="Center" 
                                                 TextBlock.FontSize="14" TextBlock.FontWeight="Bold" TextBlock.Foreground="#86b6d8"/>
