    <!--Main 대메뉴 스타일 설정-->
    <Style TargetType="Button" x:Key="MainMNUButton">
        <Setter Property="Height" Value="Auto"/>
        <Setter Property="Width" Value="250"/>
        <Setter Property="Padding" Value="0,0,0,0"/>
        <Setter Property="Foreground" Value="White"/>
        <Setter Property="FontSize" Value="15"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="Button">
                    <Border Background="#5190BC" BorderThickness="0,0,1,0" BorderBrush="#125F97">
                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                            <Image Source="{Binding  Path=Tag, RelativeSource={RelativeSource TemplatedParent}}" Stretch="None" Width="Auto" Height="Auto" Margin="0,0,5,0"/>
                            <ContentPresenter Margin="0,0,0,0" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                        </StackPanel>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Style.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="{x:Type Button}">
                            <Border Background="#2574AC" BorderThickness="0,0,1,0" BorderBrush="#125F97">
                                <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                    <Image Source="{Binding  Path=Tag, RelativeSource={RelativeSource TemplatedParent}}" Stretch="None" Width="Auto" Height="Auto" Margin="0,0,5,0"/>
                                    <ContentPresenter Margin="0,0,0,0" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                </StackPanel>
                            </Border>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Trigger>
        </Style.Triggers>
    </Style>

